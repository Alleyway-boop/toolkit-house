{
  "name": "@toolkit-house/logger",
  "type": "module",
  "version": "0.0.0",
  "description": "A lightweight, performant logging library with structured logging support",
  "author": "yuanfang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "keywords": ["logger", "logging", "typescript", "structured-logging", "performance"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs"
    },
    "./transports": {
      "types": "./dist/transports/index.d.ts",
      "import": "./dist/transports/index.mjs",
      "require": "./dist/transports/index.cjs"
    },
    "./formatters": {
      "types": "./dist/formatters/index.d.ts",
      "import": "./dist/formatters/index.mjs",
      "require": "./dist/formatters/index.cjs"
    },
    "./filters": {
      "types": "./dist/filters/index.d.ts",
      "import": "./dist/filters/index.mjs",
      "require": "./dist/filters/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild --fail-on-warn false",
    "dev": "unbuild --stub",
    "lint": "echo 'Linting skipped for logger package'",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "24.0.8",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "bumpp": "10.2.0",
    "eslint": "^9.29.0",
    "tinyexec": "1.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vite": "7.0.0",
    "vitest": "3.2.4",
    "vitest-package-exports": "0.1.1"
  }
}