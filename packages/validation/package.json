{
  "name": "@toolkit-house/validation",
  "type": "module",
  "version": "0.0.0",
  "description": "Type-safe validation library with fluent API leveraging ts-utils",
  "author": "yuanfang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "keywords": [
    "validation",
    "typescript",
    "validator",
    "schema",
    "fluent-api",
    "type-safe",
    "ts-utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.cjs"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.mjs",
      "require": "./dist/validators/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild --fail-on-warn false",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:ci": "vitest --run --reporter=basic",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "24.0.8",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "bumpp": "10.2.0",
    "eslint": "^9.29.0",
    "tinyexec": "1.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "unbuild": "3.5.0",
    "vitest": "3.2.4",
    "vitest-package-exports": "0.1.1"
  },
  "dependencies": {
    "@toolkit-house/ts-utils": "workspace:*",
    "@toolkit-house/types": "workspace:*"
  },
  "peerDependencies": {
    "@toolkit-house/constants": "workspace:*"
  }
}
